<!DOCTYPE html>
<html>
  <head>
    <title>Bookers2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <body>
    <% if user_signed_in? %>
      <li>
        <%= link_to "Booker2", home_session_path, method: :home/top %>
      </li>
        <li>
        <%= link_to "Home", home_session_path, method: :home/top %>
      </li>
      <li>
        <%= link_to "About", about_session_path, method: :home/top %>
      </li>

      <li>
        <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
      </li>
    <% else %>
      <li>
        <%= link_to "sign_up", new_user_registration_path %>
      </li>
      <li>
        <%= link_to "ログイン", new_user_session_path %>
      </li>
    <% end %>

    <%= yield %>
    <p1>CopyRight Infratop.inc</p1>
  </body>
</html>
